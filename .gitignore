# Ignore directories generated by Composer
vendor/
bin/
keys/

# Ignore core and vendor when managing dependencies with Composer.
web/core
vendor

# Ignore contrib modules
web/modules/contrib

# Ignore contrib themes
web/themes/contrib

# Ignore libraries
web/libraries

# Ignore configuration files that may contain sensitive information.

# Ignore paths that contain user-generated content.
web/sites/*/files
web/sites/*/private

# Gulp node_modules
web/themes/custom/*/node_modules/

# Minified files
web/themes/custom/*/css/min
web/themes/custom/*/js/min/*
web/themes/custom/*/assets/js
web/modules/custom/*/assets/js
web/modules/features/*/assets/js
!web/themes/custom/*/js/min/libs

# Ignore .idea (IDE files)
.idea

# Ignore local environment variables
*.env

web/*/.eslintrc
drush/contrib/*
*/.eslintrc
.eslintrc

# Ignore all dumped dbs
*--dump.sql
*.sql

# Ignore Javascript sourcemaps
*.js.map

# Ignore webpack dist folder
dist
node_modules
yarn-error.log
.vscode
/.csslintrc
/.ht.router.php
/.htaccess
/INSTALL.txt
/README.txt
/autoload.php
/example.gitignore
/index.php
/robots.txt
/update.php
/web.config
/.editorconfig
/.gitattributes

# Ignore other files/directories
/scratch
